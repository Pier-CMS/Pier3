"
Examples for the bootstrap image support
"
Class {
	#name : #TBSImageExample,
	#superclass : #TBSBootstrapExample,
	#category : #'Bootstrap-Examples-Core'
}

{ #category : #accessing }
TBSImageExample class >> exampleName [

	^'Images'
]

{ #category : #private }
TBSImageExample >> dataImage140x140 [

	^'data:image/png;base64,'
]

{ #category : #rendering }
TBSImageExample >> renderExampleOn: html [

 	html tbsImage 
		beRounded;
		url: self dataImage140x140.
	 
	html tbsImage 
		beCircle;
		url: self dataImage140x140.
		
	html tbsImage 
		beThumbnail;
		url: self dataImage140x140.		 

]
